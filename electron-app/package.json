{
  "name": "do-it-now",
  "version": "0.0.0",
  "private": true,
  "description": "To Do App in electron with server save",
  "author": {
    "name": "Ahmed Marzook"
  },
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "build": "vite build && npm run transpile:electron",
    "build-storybook": "storybook build",
    "check": "prettier --write . && eslint --fix",
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:electron": "npm run transpile:electron && cross-env NODE_ENV=development electron .",
    "dev:react": "vite",
    "dist:linux": "npm run transpile:electron && npm run build && electron-builder --config electron-builder.json --linux --x64",
    "dist:mac": "npm run transpile:electron && npm run build && electron-builder --config electron-builder.json --mac --arm64",
    "dist:win": "npm run transpile:electron && npm run build && electron-builder --config electron-builder.json --win --x64",
    "format": "prettier",
    "postinstall": "electron-builder install-app-deps && npm run rebuild",
    "lint": "eslint",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "serve": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "transpile:electron": "tsc --project src/electron/tsconfig.json && tsc-alias -p src/electron/tsconfig.json"
  },
  "dependencies": {
    "@storybook/react-vite": "^9.1.9",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron": "^4.3.5",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^9.1.9",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/electron": "^1.4.38",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^10.1.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "jsdom": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}
