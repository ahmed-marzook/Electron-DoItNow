# Environment Configuration Example
# Copy this file to .env.local, .env.preprod, or .env.production

# Environment mode
NODE_ENV=development

# API Configuration (Renderer - prefixed with VITE_)
VITE_API_BASE_URL=http://localhost:8080
VITE_API_TIMEOUT=10000

# API Configuration (Main Process)
API_BASE_URL=http://localhost:8080
API_TIMEOUT=10000

# Sync Configuration
SYNC_INTERVAL_CRON=* * * * *
SYNC_TIMEZONE=America/New_York
ENABLE_AUTO_SYNC=true
